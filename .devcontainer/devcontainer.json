{
    "name": "MUF Development Environment",
    "dockerComposeFile": [
        "../docker-compose.yml",
        "docker-compose.extend.yml"
    ],
    "service": "muf-echo-unit",
    "workspaceFolder": "/workspaces/MUF",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-python.vscode-pylint",
                "ms-azuretools.vscode-docker",
                "tamasfe.even-better-toml"
            ],
            "settings": {
                "python.analysis.extraPaths": [
                    "/workspaces/MUF"
                ]
            }
        }
    },
    "remoteUser": "root"
}